{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.13.7/.schema/devbox.schema.json",
  "packages": [
    "kind@latest",
    "docker@latest",
    "jq@latest",
    "yq@latest",
    "git@latest",
    "k9s@latest",
    "tilt@latest",
    "awscli2@latest",
    "direnv@latest",
    "kustomize@5.5.0",
    "go@1.24.1"
  ],
  "shell": {
    "init_hook": [
      "export PATH=$PWD/bin:$PATH",
      "./scripts/devbox-init-hook.sh"
    ],
    "scripts": {
      "clean": [
        "make clean"
      ],
      "build": [
        "make generate && make clusterawsadm"
      ]
    }
  }
}
